{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:all": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && rm -fr package-loca.json && rm -fr yarn.lock && npm cache clean && npm cache verify",
    "purge": "yarn clean:all && yarn",
    "start": "node node_modules/react-native/local-cli/cli.js start  -- --reset-cache",
    "test": "jest",
    "prettier": "prettier --write --single-quote --no-semi --trailing-comma es5 --print-width 80 \"app/**/*.js\"",
    "lint": "eslint app",
    "format": "yarn prettier && yarn lint --fix",
    "openDebugger": "open /Applications/\"React Native Debugger.app\"",
    "ios": "concurrently \"npm run openDebugger\" \"npm start\" \"node node_modules/react-native/local-cli/cli.js run-ios  --simulator 'iPhone 7'\"",
    "emulator": "cd /Users/hexiao/Library/Android/sdk/emulator && ./emulator @Nexus_5X_API_27",
    "android": "concurrently \"npm run openDebugger\"  \"npm start\"  \"npm run emulator\" \"node node_modules/react-native/local-cli/cli.js run-android\" "
  },
  "dependencies": {
    "dva-core": "^1.4.0",
    "metro-react-native-babel-preset": "^0.51.1",
    "react": "^16.6.3",
    "react-native": "^0.57.4",
    "react-navigation": "^2.17.0",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.7"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "^5.0.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.1",
    "schedule": "^0.5.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
