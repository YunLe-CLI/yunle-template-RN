{
  "name": "DvaStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:all": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean && npm cache verify",
    "purge": "yarn clean:all && yarn",
    "start": "node node_modules/react-native/local-cli/cli.js start  -- --reset-cache",
    "test": "jest",
    "prettier": "prettier --write --single-quote --no-semi --trailing-comma es5 --print-width 80 \"app/**/*.js\"",
    "lint": "eslint app",
    "format": "yarn prettier && yarn lint --fix",
    "precommit": "yarn format"
  },
  "dependencies": {
    "dva-core": "^1.4.0",
    "metro-react-native-babel-preset": "^0.45.6",
    "react": "^16.5.2",
    "react-native": "^0.57.1",
    "react-navigation": "^2.16.0",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.7"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-native": "^5.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.5.2",
    "schedule": "^0.4.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
